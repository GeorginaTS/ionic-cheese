{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/*.js",
      "/*.css",
      "/*.png",
      "/*.svg",
      "/*.ico",
      "/*.json"
    ]
  },
  "routes": [
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ],
  "platform": {
    "apiRuntime": "node:20"
  },
  "mimeTypes": {
    ".js": "application/javascript",
    ".css": "text/css",
    ".html": "text/html",
    ".json": "application/json",
    ".png": "image/png",
    ".svg": "image/svg+xml",
    ".ico": "image/x-icon"
  },
  "trailingSlash": "auto"
}
