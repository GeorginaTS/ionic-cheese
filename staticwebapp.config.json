{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/*.js",
      "/*.css",
      "/*.png",
      "/*.svg",
      "/*.ico",
      "/*.json"
    ]
  },
  "routes": [
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ],
  "platform": {
    "apiRuntime": "node:20"
  },
  "trailingSlash": "auto"
}

